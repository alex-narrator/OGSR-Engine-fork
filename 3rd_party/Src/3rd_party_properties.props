    <OutDir>$(SolutionDir)\bin_$(PlatformShortName)\</OutDir>
    <IntDir>$(SolutionDir)\3rd_party\_TEMP\$(Configuration)\$(Platform)\$(ProjectName)\</IntDir>
  </PropertyGroup>

  <ItemDefinitionGroup>
